package packagea;
import packageb.ClassC;

public class ClassA{
	/**
	 * This method adds two numbers and returns the result in the form of {@code int} or <code> int </code>
	 * @param x int value
	 * @param y int value
	 * @return int representation of the result
	 */
	public int add(int x, int y) {
		return x+y;

	}
		public static void main(String[] args) {
			System.out.println("Hello");
			ClassA a = new ClassA();
			a.add(10,20);
			
		
		
	}
}

//Access modifiers==>
//public ==>  method variable(static/instance not local)and class and it can be called by using any object
//Private==> method variable but not class(Class can never be private)
//default==> accessible inside one package only, used for method, variable and class
//main method always inside public class
//protected==> accessible inside same package/child class, method and variable

//visibility of access modifiers==> low to high is as follows:-
/* 	private
	default
	protected
	public		*/

